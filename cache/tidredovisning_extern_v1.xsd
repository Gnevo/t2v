<?xml version="1.0" encoding="UTF-8"?><xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ase="http://schema.forsakringskassan.se/integration/isassistansersattning/tidredovisning/1" targetNamespace="http://schema.forsakringskassan.se/integration/isassistansersattning/tidredovisning/1" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.8">	<xsd:annotation>		<xsd:documentation xml:lang="sv">Beskrivning av information vid inskick av tidredovisning</xsd:documentation>	</xsd:annotation>	<!-- 	  Andringshistorik. Nar, Version, Vem, Andring.	  2016-04-10: 1.0, FK, Initial 	  2016-05-19: 1.0.1, FK, Borttag av element datumForUnderskriftAnordnare och datumForUnderskriftAssistent						Ändrat Typen Organisationsnummer till 10 siffror						Förtydligat att Samordningsnummer inte stöds av Bankid i nuvarande version. Ligger kvar för framtida bruk.	  2016-06-20: 1.0.2 FK, Lagt till en simpletype enum som heter True, och ändrat till denna istället för xsd:boolean för fälten assistentAnstalldAvAnordnare, assistentEgenForetagare,aktivTid, vanteTid, beredskapsTid samt anstalldAvPersonSomHarAssistans	 	-->	<xsd:simpleType name="UUID">		<xsd:restriction base="xsd:string">			<xsd:pattern value="[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="Personnummer">		<xsd:restriction base="xsd:token">			<xsd:pattern value="\d{12}">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Personnummer på format YYYYMMDDNNNN. Exempel 197503231111</xsd:documentation>				</xsd:annotation>			</xsd:pattern>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="Samordningsnummer">		<xsd:restriction base="xsd:token">			<xsd:pattern value="\d{12}">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Samordningsnummer på format YYYYMM(DD+60)NNNN, Exempel 197503831111</xsd:documentation>				</xsd:annotation>			</xsd:pattern>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="Organisationsnummer">		<xsd:restriction base="xsd:token">			<xsd:pattern value="\d{10}">				<xsd:annotation>					<xsd:documentation xml:lang="sv">10 siffrigt organisationsnummer skall användas</xsd:documentation>				</xsd:annotation>			</xsd:pattern>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="Redovisningsmanad">		<xsd:restriction base="xsd:token">			<xsd:pattern value="\d{4}-\d{2}"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="Klockslag">		<xsd:restriction base="xsd:token">			<xsd:pattern value="\d{2}\.\d{2}"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="Dag">		<xsd:restriction base="xsd:integer">			<xsd:minInclusive value="1"/>			<xsd:maxInclusive value="31"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:complexType name="PersonSomHarAssistans">		<xsd:sequence>			<xsd:element name="personnummer" type="ase:Personnummer"/>			<xsd:element name="fornamn" type="xsd:string"/>			<xsd:element name="efternamn" type="xsd:string"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="Assistent">		<xsd:sequence>			<xsd:choice>				<xsd:element name="personnummer" type="ase:Personnummer"/>				<xsd:element name="samordningsnummer" type="ase:Samordningsnummer">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Samordningsnummer stöds inte av Bankid i nuläget. Ligger kvar för framtida möjligheter</xsd:documentation>					</xsd:annotation>				</xsd:element>			</xsd:choice>			<xsd:element name="fornamn" type="xsd:string"/>			<xsd:element name="efternamn" type="xsd:string"/>			<xsd:element name="kollektivavtal" type="xsd:boolean">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Sätts till true om Assistenten omfattas av kollektivavtal. </xsd:documentation>				</xsd:annotation>			</xsd:element>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="Berakningsperiod">		<xsd:sequence>			<xsd:element name="fromDag" type="xsd:date"/>			<xsd:element name="tomDag" type="xsd:date"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="AnnanArbetsgivare">		<xsd:sequence>			<xsd:element name="arbetsgivareNamn" type="xsd:string" minOccurs="0">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Arbetsgivarens namn.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="organisationsnummer" type="ase:Organisationsnummer"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="Anordnare">		<xsd:sequence>			<xsd:element name="anordnareNamn" type="xsd:string" minOccurs="0">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Anordnarens namn.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="organisationsnummer" type="ase:Organisationsnummer"/>			<xsd:element name="kontaktperson" type="xsd:string" minOccurs="0">				<xsd:annotation>					<xsd:documentation xml:lang="sv">För- och Efternman på den person som Försäkringskassans handläggare kan kontakta om det finns behov av komplettering. .</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="telefonnummer" type="ase:Telefonnummer" minOccurs="0"/>			<xsd:choice>				<xsd:element name="assistentAnstalldAvAnordnare" type="ase:True">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Sätts till true om assistenten är anställd av anordnaren. </xsd:documentation>					</xsd:annotation>				</xsd:element>				<xsd:element name="annanArbetsgivare" type="ase:AnnanArbetsgivare">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Elementet beskriver om assistenten är anställd av annan arbetsgivare än anordnaren. </xsd:documentation>					</xsd:annotation>				</xsd:element>				<xsd:element name="assistentEgenForetagare" type="ase:True">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Sätts till true om assistenten är egen företagare. </xsd:documentation>					</xsd:annotation>				</xsd:element>			</xsd:choice>		</xsd:sequence>	</xsd:complexType>	<xsd:simpleType name="True">		<xsd:restriction base="xsd:string">			<xsd:enumeration value="true">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Enumeration för att sätta true på vissa fält</xsd:documentation>				</xsd:annotation>			</xsd:enumeration>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="Telefonnummer">		<xsd:restriction base="xsd:string">			<xsd:pattern value=".{7,}"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:complexType name="Tidrapport">		<xsd:sequence>			<xsd:element name="rapportRad" type="ase:RapportRad" minOccurs="0" maxOccurs="unbounded"/>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="RapportRad">		<xsd:sequence>			<xsd:element name="dag" type="ase:Dag"/>			<xsd:element name="fromTid" type="ase:Klockslag"/>			<xsd:element name="tomTid" type="ase:Klockslag"/>			<xsd:choice>				<xsd:element name="aktivTid" type="ase:True">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Sätts till true om tiden avser normal arbetstid. </xsd:documentation>					</xsd:annotation>				</xsd:element>				<xsd:element name="vanteTid" type="ase:True">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Sätts till true om tiden avser väntetid </xsd:documentation>					</xsd:annotation>				</xsd:element>				<xsd:element name="beredskapsTid" type="ase:True">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Sätts till true om tiden avser beredskapstid</xsd:documentation>					</xsd:annotation>				</xsd:element>			</xsd:choice>		</xsd:sequence>	</xsd:complexType>	<xsd:complexType name="Tidredovisning">		<xsd:sequence>			<xsd:element name="id" type="ase:UUID">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Ett unikt id för tidredovisningen.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="redovisningsmanad" type="ase:Redovisningsmanad">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Den månad som tisdsredovisningen avser. Format YYYY-MM. Exempel 2015-03 </xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="personSomHarAssistans" type="ase:PersonSomHarAssistans">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Personen som får assistans.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="assistent" type="ase:Assistent">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Personen som utför assistansen.</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:element name="berakningsperiod" type="ase:Berakningsperiod" minOccurs="0" maxOccurs="unbounded">				<xsd:annotation>					<xsd:documentation xml:lang="sv">Period som begränsar arbetstiden .</xsd:documentation>				</xsd:annotation>			</xsd:element>			<xsd:choice>				<xsd:element name="anstalldAvPersonSomHarAssistans" type="ase:True">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Sätts till true om Personen som får assistans själv har anställt assistenten.</xsd:documentation>					</xsd:annotation>				</xsd:element>				<xsd:element name="assistansanordnare" type="ase:Anordnare">					<xsd:annotation>						<xsd:documentation xml:lang="sv">Väljs om Personen som får assistans anlitar en assistansanordnare.</xsd:documentation>					</xsd:annotation>				</xsd:element>			</xsd:choice>			<xsd:element name="tidrapport" type="ase:Tidrapport"/>			<xsd:element name="telefonnummerAnordnare" type="ase:Telefonnummer"/>			<xsd:element name="telefonnummerAssistent" type="ase:Telefonnummer" minOccurs="0"/>		</xsd:sequence>	</xsd:complexType>	<xsd:element name="Tidredovisning" type="ase:Tidredovisning"/></xsd:schema>